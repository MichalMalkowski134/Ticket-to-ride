        "from PIL import Image\n",
        "def augmenting_image(path, format):\n",
        "  Orig = Image.open(path)\n",
        "  for i in range(1,7):\n",
        "    New_Img = Orig.rotate(60*i)\n",
        "    New_Img.save(path+\".\"+format)"
      ],
